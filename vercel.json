{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/submit",
        "methods": ["POST"],
        "dest": "/index.js"
      },
      {
        "src": "/",
        "methods": ["GET"],
        "dest": "/index.js"
      },
      {
        "src": "/delete/(.*)",
        "methods": ["POST"],
        "dest": "/index.js"
      },
      {
        "src": "/edit/(.*)",
        "methods": ["GET"],
        "dest": "/index.js"
      },
      {
        "src": "/edit/(.*)",
        "methods": ["POST"],
        "dest": "/index.js"
      }
      
    ]
  }
  